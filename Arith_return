def add(n1,n2):
    return n1+n2
def sub(n1, n2):
    return n1-n2
def mul(n1, n2):
    return n1*n2
def division(n1, n2):
    return n1/n2
n1 = int(input("Enter the First Value  = "))
n2 = int(input("Enter the Second Value = "))
print("Addition")
print(add(n1, n2))
print("Subtraction")
print(sub(n1, n2))
print("Multiplication") 
print(mul(n1, n2))
print("Division")
print(division(n1, n2))
